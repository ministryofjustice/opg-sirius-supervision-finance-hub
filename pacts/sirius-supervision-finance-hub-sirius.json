{
  "consumer": {
    "name": "sirius-supervision-finance-hub"
  },
  "interactions": [
    {
      "description": "A request for client",
      "providerState": "A supervision client exists",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "\\/supervision-api\\/v1\\/clients\\/\\d+"
          }
        },
        "method": "GET",
        "path": "/supervision-api/v1/clients/123"
      },
      "response": {
        "body": {
          "caseRecNumber": "22222222",
          "firstname": "Ian",
          "id": 1,
          "surname": "Finance"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.caseRecNumber": {
            "match": "type"
          },
          "$.body.firstname": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.surname": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "A request for client",
      "providerState": "Client exists",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "\\/supervision-api\\/v1\\/clients\\/\\d+"
          }
        },
        "method": "GET",
        "path": "/supervision-api/v1/clients/1"
      },
      "response": {
        "body": {
          "caseRecNumber": "22222222",
          "firstname": "Ian",
          "id": 1,
          "surname": "Finance"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.caseRecNumber": {
            "match": "type"
          },
          "$.body.firstname": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.surname": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "A request for the current user",
      "providerState": "User exists",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/supervision-api/v1/users/current"
      },
      "response": {
        "body": {
          "displayName": "Colin Case",
          "id": 1,
          "roles": [
            "Case Manager"
          ]
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.displayName": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.roles": {
            "match": "type",
            "min": 1
          }
        },
        "status": 200
      }
    },
    {
      "description": "A request for users",
      "providerState": "Users exists",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/supervision-api/v1/users"
      },
      "response": {
        "body": [
          {
            "displayName": "Colin Case",
            "id": 1,
            "roles": [
              "Case Manager"
            ]
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type",
            "min": 1
          },
          "$.body[*].displayName": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "type"
          },
          "$.body[*].roles": {
            "match": "type",
            "min": 1
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-go": {
      "version": "2.4.1"
    },
    "pactRust": {
      "ffi": "0.4.27",
      "mockserver": "1.2.11",
      "models": "1.2.8"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "sirius"
  }
}
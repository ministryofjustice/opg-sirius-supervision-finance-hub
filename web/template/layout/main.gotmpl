{{- /*gotype: github.com/ministryofjustice/opg-sirius-supervision-finance-hub/internal/server.PageData*/ -}}
{{ define "main" }}
    <div class="govuk-width-container app-width-container">
        <main class="govuk-main-wrapper app-main-class" role="main">
            {{ template "person-info" .Person }}
            <div id="error-summary">
                {{/* template must be included to find the partial but false passed in to prevent it rendering */}}
                {{ template "error-summary" false }}
            </div>
            <div id="main-content">
                {{ template "navigation" . }}
                <div id="tab-content">
                    {{ template "tab-content" . }}
                </div>
            </div>
        </main>
    </div>
{{ end }}
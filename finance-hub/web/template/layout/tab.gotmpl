{{ define "tab" }}
    <li class="moj-sub-navigation__item"
        data-cy="{{ .Id }}"
    >
        <div
                class="moj-sub-navigation__link"
                hx-get="{{ prefix .BasePath }}"
                hx-target="#tab-content"
                hx-push-url="{{ .BasePath }}"
        >
            {{ .Title }}
        </div>
    </li>
{{ end }}
